{
	"name": "GBF Battle ID Copy",
	"version": "1.0.5",
	"minimum_chrome_version": "40.0",
	"manifest_version": 2,
	"description": "Adds a copy to clipboard button for the multibattle ID in Granblue Fantasy.",
	"content_scripts": [
		{
			"matches": ["http://gbf.game.mbga.jp/*", "http://game.granbluefantasy.jp/*"],
			"js": ["src/content/copy.js"]
		}
	],
	"permissions": [
		"http://gbf.game.mbga.jp/*",
		"http://game.granbluefantasy.jp/*"
	]
}
